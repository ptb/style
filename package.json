{
  "author": {
    "email": "peter@ptb.dev",
    "name": "Peter T Bosse II",
    "url": "https://ptb.dev/"
  },
  "ava": {
    "extensions": [
      "js"
    ],
    "failFast": false,
    "files": [
      "src/**/*.test.js"
    ],
    "verbose": true
  },
  "babelMacros": {
    "@ptb/style": {
      "output": "css/index.css"
    }
  },
  "bin": {
    "merge-json": "./merge-json.cjs"
  },
  "bugs": {
    "url": "https://github.com/ptb/style/issues"
  },
  "c8": {
    "extension": [
      ".js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "temp-directory": "/tmp/c8_output"
  },
  "description": "@ptb/style is a CSS compiler with a powerful, intuitive API that makes authoring CSS faster and easier.",
  "devDependencies": {
    "@ava/babel": "2.0.0",
    "@rollup/plugin-replace": "4.0.0",
    "@rollup/plugin-strip": "2.1.0",
    "@rollup/plugin-typescript": "8.3.1",
    "@size-limit/file": "7.0.8",
    "@types/babel-plugin-macros": "2.8.5",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "ava": "4.1.0",
    "babel-plugin-macros": "3.1.0",
    "c8": "7.11.0",
    "coveralls": "3.1.1",
    "csstype": "3.0.11",
    "dts-bundle-generator": "6.5.0",
    "eslint": "7.32.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "38.0.4",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-standard": "5.0.0",
    "gatsby": "4.9.3",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.8",
    "prettier-eslint": "13.0.0",
    "prettier-eslint-cli": "5.0.1",
    "puppeteer": "13.5.1",
    "rollup": "2.70.1",
    "rollup-plugin-execute": "1.1.1",
    "rollup-plugin-terser": "7.0.2",
    "serve": "13.0.2",
    "size-limit": "7.0.8",
    "tslib": "2.3.1",
    "typescript": "4.6.2",
    "vite": "2.8.6",
    "vite-plugin-macro": "0.2.0"
  },
  "eslintIgnore": [
    "style.*",
    "!src/**/*.js"
  ],
  "exports": {
    ".": {
      "import": "./style.js",
      "require": "./macro/style.js"
    },
    "./index": "./index.js",
    "./macro": "./macro/index.js",
    "./package.json": "./package.json",
    "./vite-plugin": "./vite-plugin.cjs"
  },
  "files": [
    "gatsby-ssr.js",
    "index.d.ts",
    "index.js",
    "macro/index.js",
    "macro/index.d.ts",
    "macro/package.json",
    "macro/style.js",
    "macro/style.js.map",
    "merge-json.cjs",
    "style.js",
    "style.js.map",
    "vite-plugin.cjs"
  ],
  "keywords": [
    "babel-plugin-macros",
    "css",
    "css-in-js",
    "json",
    "merge",
    "react",
    "styles"
  ],
  "license": "Apache-2.0",
  "main": "macro/style.js",
  "maintainers": [
    {
      "email": "peter@ptb.dev",
      "name": "Peter T Bosse II",
      "url": "https://ptb.dev/"
    }
  ],
  "module": "style.js",
  "name": "@ptb/style",
  "nodemonConfig": {
    "delay": 2500,
    "events": {
      "start": "echo \"\\x1Bc\""
    },
    "exec": "npm-run-all clean lint tidy test prepare build strip",
    "ext": "cjs js json jsx ts",
    "verbose": true,
    "watch": [
      "package.json",
      "src"
    ]
  },
  "packageManager": "yarn@3.1.1",
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "parser": "babel",
    "printWidth": 70,
    "proseWrap": "always",
    "requirePragma": false,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "publishConfig": {
    "access": "public"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "ignoreDeps": [
      "eslint",
      "yarn"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ptb/style"
  },
  "scripts": {
    "build": "tsc -p jsconfig.json && dts-bundle-generator --no-banner --out-file index.d.ts --project jsconfig.json src/style.js 2> /dev/null || true && rm -rf types 2> /dev/null || true",
    "clean": "rm -rf gatsby-ssr.js index.js macro/index.js macro/style.js* merge-json.cjs style.js* types vite-plugin.cjs 2> /dev/null || true",
    "lint": "eslint --fix src",
    "prepare": "rollup -c && size-limit",
    "strip": "sed -i'' -E -e 's/export type Params = Params;//' -e 's/export type PlainObject = PlainObject;//' -e 's/export type StylesObject = StylesObject;//' -e 's/`@media \\$\\{string}`/\"@media ${string}\"/' -e 's/`\\$\\{number}%`/\"${number}%\"/' -e 's/`%\\$\\{string}`/\"%${string}\"/' -e 's/`\\$\\$\\{string}`/\"$${string}\"/' -e 's/\\\\t/  /g' -e 's/ \\\\* /  /' -e 's/ *[@]typedef [{]import.*//' -e 's/\\\\/[*][*]/\\\\n\\\\n\\\\/**/' -e 's/[*]\\\\//*\\\\/\\\\n/' -e 's/export/\\\\nexport/' -e 's/export \\{};//' index.d.ts && sed -i'' -E -e '/^$/N;/^\\\\n$/D' index.d.ts && prettier-eslint --parser \"typescript\" --semi --write index.d.ts && sed -i'' -E -e 's/      \\\\/[*][*]/  \\\\/**/' -e 's/\\\"@media \\$\\{string}\\\"/`@media ${string}`/' -e 's/\\\"\\$\\{number}%\\\"/`${number}%`/' -e 's/\\\"\\$\\$\\{string}\\\"/`$${string}`/' -e 's/\\\"%\\$\\{string}\\\"/`%${string}`/' index.d.ts || true",
    "test": "c8 ava",
    "tidy": "prettier-eslint --write \"src/**/*.js\"",
    "watch": "nodemon"
  },
  "sideEffects": false,
  "size-limit": [
    {
      "limit": "9 KB",
      "path": "style.js"
    },
    {
      "limit": "9 KB",
      "path": "macro/style.js"
    },
    {
      "limit": "9.5 KB",
      "path": "index.js"
    }
  ],
  "type": "module",
  "types": "index.d.ts",
  "version": "0.9.19"
}
